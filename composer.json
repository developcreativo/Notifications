{
    "name": "developcreativo/notifications",
    "description": "A Laravel Nova tool to display user notifications.",
    "keywords": [
        "laravel",
        "nova",
        "faceit"
    ],
    "authors": [
        {
            "email": "developmentcreativo@gmail.com",
            "name": "Kelly Salazar",
            "homepage": "https://developmentcreativo.net"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "nesbot/carbon": "^2.00",
        "illuminate/contracts": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Developcreativo\\Notifications\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Developcreativo\\Notifications\\NotificationsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
